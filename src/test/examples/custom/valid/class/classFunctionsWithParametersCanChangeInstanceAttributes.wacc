# Simple Class Function with Parameters

begin
  class Vehicle is
    private int name ;
    public int tyres 
    public int getName() is 
      return name
    end
    public int getNumber() is
      return tyres
    end
    public int setNumber(int offset) is
      tyres = tyres + offset ;
      return tyres
    end
  end
  Vehicle bus = new Vehicle(7, 6) ;
  Vehicle car = new Vehicle(6, 4) ;
  println bus.tyres ;
  println car.tyres ;
  int v = call bus.setNumber(5) ;
  println v ;
  println bus.tyres 
end








